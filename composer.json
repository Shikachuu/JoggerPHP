{
    "name": "shikachuu/jogger",
    "version": "0.1.3",
    "time": "2020-10-25",
    "type": "library",
    "description": "PSR-3 compatible logger library for PHP mostly based on a Go project rs/zerolog.",
    "keywords": [
        "log",
        "logging",
        "psr-3",
        "jogger"
    ],
    "homepage": "https://github.com/Shikachuu/JoggerPHP",
    "readme": "README.md",
    "license": "Boost Software License",
    "authors": [
        {
            "name": "Shikachuu",
            "email": "shikachu@pm.me"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Jogger\\": "src"
        }
    },
    "require": {
        "php": ">=7.4",
        "psr/log": "^1.1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "platform-check": false,
        "optimize-autoloader": true
    },
    "scripts": {
        "test": "phpunit --coverage-html coverage --stop-on-failure --fail-on-risky --testdox --strict-coverage",
        "lint": "phpcs . -n",
        "lint-fix": "phpcbf . -n"
    },
    "scripts-descriptions": {
        "test": "Run all unit tests!",
        "lint": "Run PHP_CodeSniffer to analyze the code!",
        "lint-fix": "Run PHP_CodeSniffer to fix the code!"
    }
}